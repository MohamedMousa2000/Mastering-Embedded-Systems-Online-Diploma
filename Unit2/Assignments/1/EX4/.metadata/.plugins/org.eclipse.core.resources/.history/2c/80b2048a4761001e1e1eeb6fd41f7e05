 Description : Multiply two Floating Point Numbers
 ============================================================================
 */

#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int x = 0;
	int y = 0;
	printf("Enter two numbers: ");
	fflush(stdin); fflush(stdout);
	scanf("%d %d", &x, &y);
	printf("Product: %d", x * y);
	return EXIT_SUCCESS;
}
