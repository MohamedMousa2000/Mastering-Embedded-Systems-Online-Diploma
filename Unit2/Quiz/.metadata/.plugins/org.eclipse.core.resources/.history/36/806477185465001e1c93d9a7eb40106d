#include <stdio.h>
#include <stdlib.h>

int main(void) {
	unsigned short i = 3;
	switch(i)
	{
		printf("DDDDDDDDDD");
		case 0: printf("CCCCCCCCC");break;
		case 1+2: printf("AAAAAAAAA");break;
		default:printf("RRRRRRRRR");
	}
	return EXIT_SUCCESS;
}
