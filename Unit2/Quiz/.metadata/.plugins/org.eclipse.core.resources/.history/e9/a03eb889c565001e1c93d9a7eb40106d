#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void)
{
	int a, b;
	for(a = 0, b = 0; a < 10, b < 5; a++, b++)
	{
		printf("%d %d\n",a,b);
	}

	return EXIT_SUCCESS;
}
