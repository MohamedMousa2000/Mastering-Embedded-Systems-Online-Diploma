#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void)
{
	long double a;
	signed char b;
	int arr[sizeof(!a+b)];
	printf("%d\n", sizeof(arr));
	return EXIT_SUCCESS;
}
