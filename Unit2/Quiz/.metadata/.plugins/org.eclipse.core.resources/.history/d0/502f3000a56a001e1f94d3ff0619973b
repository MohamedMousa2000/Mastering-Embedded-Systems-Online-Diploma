#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int Check(char arr[], char username[]);

int main(void)
{
	char username[8] = "Mohamed";
	char arr[8];
	printf("Enter string: ");
	fflush(stdin); fflush(stdout);
	gets(arr);
	if(Check(arr, username))
	{
		printf("VALID\n");
	}
	else
	{
		printf("Invalid!!\n");
	}

	return EXIT_SUCCESS;
}

int Check(char arr[], char username[])
{
	int i = strcmp(arr, username);
	return i;
}
